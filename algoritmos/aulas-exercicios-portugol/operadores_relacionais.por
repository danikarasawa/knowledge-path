programa {
    
    inclua biblioteca Matematica --> m
	funcao inicio() {
	    inteiro A, B, C
	    
	    A = 2
	    B = 3
	    C = 5
	    
	    escreva(A>B) //falso
	    escreva(A=B) //falso
	    escreva(A<>B) //verdadeiro
	    escreva(A>2) //falso
	    escreva(A>=2) //verdadeiro
	    escreva(C=A+B) //verdadeiro
	    escreva(C>= m.potencia(B,A)) //falso
	    escreva(B<=C%2) //falso
	}
}
